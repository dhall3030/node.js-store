<div class="pagination">
			  <ul>
			    {{#paginate pagination type="first"}}
			      <li {{#if disabled}}class="disabled"{{/if}}><a href="{{path}}{{n}}">First</a></li>
			    {{/paginate}}
			    {{#paginate pagination type="previous"}}
			      <li {{#if disabled}}class="disabled"{{/if}}><a href="{{path}}{{n}}">Prev</a></li>
			    {{/paginate}}
			    {{#paginate pagination type="middle" limit="7"}}
			      <li {{#if active}}class="active"{{/if}}><a href="{{path}}{{n}}">{{n}}</a></li>
			    {{/paginate}}
			    {{#paginate pagination type="next"}}
			      <li {{#if disabled}}class="disabled"{{/if}}><a href="{{path}}{{n}}">Next</a></li>
			    {{/paginate}}
			    {{#paginate pagination type="last"}}
			      <li {{#if disabled}}class="disabled"{{/if}}><a href="{{path}}{{n}}">Last</a></li>
			    {{/paginate}}
			  </ul>
</div>